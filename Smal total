--uwu lol antifling bro fuck me

loadstring(game:HttpGet("https://raw.githubusercontent.com/Flashtynooo/Roblox/refs/heads/Fighting-style/Antifling"))()

local plr = game:GetService("Players").LocalPlayer
local char = plr.Character or plr.CharacterAdded:Wait()
local hum = char:WaitForChild("Humanoid")
local pack = plr:WaitForChild("Backpack")
local uis = game:GetService("UserInputService")

-- R15 check
if hum.RigType == Enum.HumanoidRigType.R15 then
	warn("This tool only works with R6 avatars.")
	local message = Instance.new("Message", workspace)
	message.Text = "‚ùå R15 is not supported. Switch to R6."
	task.delay(4, function() message:Destroy() end)
	return
end

-- Clean old
if workspace:FindFirstChild("aaa") then
	workspace:FindFirstChild("aaa"):Destroy()
end

-- Animation
local animIDs = {"rbxassetid://313762630", "rbxassetid://248336677"}
local anim = hum:WaitForChild("Animator")
local animations = {}
for i, id in ipairs(animIDs) do
	local animation = Instance.new("Animation")
	animation.AnimationId = id
	animation.Name = "aaa_" .. i
	animation.Parent = workspace
	table.insert(animations, animation)
end

-- Tool
local tool = Instance.new("Tool")
tool.Name = "Fe floating true-flshty (R6)"
tool.RequiresHandle = false
tool.Parent = pack
tool.ToolTip = "flshty_user fighting awake :)"

-- GUI
local screenGui = Instance.new("ScreenGui", game.CoreGui)
screenGui.Name = "FlshtyButtons"
screenGui.ResetOnSpawn = false
screenGui.Enabled = false

local buttonFrame = Instance.new("Frame", screenGui)
buttonFrame.Size = UDim2.new(0, 60, 0, 120)
buttonFrame.Position = UDim2.new(1, -70, 0.5, -60)
buttonFrame.BackgroundTransparency = 1

-- Boutons
local function createButton(name, key)
	local button = Instance.new("TextButton", buttonFrame)
	button.Size = UDim2.new(1, 0, 0, 50)
	button.BackgroundColor3 = Color3.new(1, 1, 1)
	button.BackgroundTransparency = 0.7
	button.BorderColor3 = Color3.new(1, 1, 1)
	button.BorderSizePixel = 3
	button.Text = key
	button.TextColor3 = Color3.new(1, 1, 1)
	button.TextScaled = true
	button.Font = Enum.Font.GothamBold
	local corner = Instance.new("UICorner", button)
	corner.CornerRadius = UDim.new(0, 25)
	return button
end

local buttonE = createButton("ButtonE", "E")
buttonA.Position = UDim2.new(0, 0, 0, 60)

local buttonZ = createButton("ButtonZ", "Z")
buttonZ.Position = UDim2.new(0, 0, 0, 0)

-- Cooldown
local cooldownTime = 2
local cooldownE = false
local cooldownZ = false

local function executeLoadstringE()
	if cooldownE or not screenGui.Enabled then return end
	cooldownE = true
	pcall(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Flashtynooo/Roblox/refs/heads/Fighting-style/Finder"))()
	end)
	task.delay(cooldownTime, function() cooldownE = false end)
end

local function executeLoadstringZ()
	if cooldownZ or not screenGui.Enabled then return end
	cooldownZ = true
	pcall(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Flashtynooo/Roblox/refs/heads/Fighting-style/Z"))()
	end)
	task.delay(cooldownTime, function() cooldownZ = false end)
end

-- Clicks
buttonA.MouseButton1Click:Connect(executeLoadstringA)
buttonZ.MouseButton1Click:Connect(executeLoadstringZ)

-- Clavier (optionnel pour PC)
uis.InputBegan:Connect(function(input, gp)
	if gp or not screenGui.Enabled then return end
	if input.KeyCode == Enum.KeyCode.A then executeLoadstringA() end
	if input.KeyCode == Enum.KeyCode.Z then executeLoadstringZ() end
end)

-- Animation Tool
local animTracks = {}

tool.Equipped:Connect(function()
	screenGui.Enabled = true
	-- Fling ON
	pcall(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Flashtynooo/Roblox/refs/heads/Fighting-style/Fling%20on"))()
	end)
	for _, animObj in ipairs(animations) do
		local track = anim:LoadAnimation(animObj)
		track:Play()
		track:AdjustSpeed(0.5)
		track.TimePosition = 0.5
		table.insert(animTracks, track)
	end
end)

tool.Unequipped:Connect(function()
	screenGui.Enabled = false
	-- Fling OFF
	pcall(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Flashtynooo/Roblox/refs/heads/Fighting-style/Fling%20off"))()
	end)
	for _, track in ipairs(animTracks) do
		if track then track:Stop() track:Destroy() end
	end
	animTracks = {}
end)

hum.Died:Connect(function()
	screenGui:Destroy()
	for _, track in ipairs(animTracks) do
		if track then track:Stop() track:Destroy() end
	end
	animTracks = {}
end)
