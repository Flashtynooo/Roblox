-- Fonction pour tuer ton propre personnage
local function killMyself()
    local player = game.Players.LocalPlayer
    local character = player.Character
    
    if character then
        -- DÃ©truire les joints du personnage
        for _, part in ipairs(character:GetChildren()) do
            if part:IsA("BasePart") then
                part:BreakJoints() -- Supprimer les joints pour tuer le joueur
            end
        end
    end
end

-- Appeler la fonction pour tuer le joueur
killMyself()
