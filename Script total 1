




local player = game.Players.LocalPlayer
local CoreGui = Game:GetService("CoreGui")
if CoreGui:FindFirstChild("Flshty_TrollHub") then
    CoreGui.Flshty_TrollHub:Destroy()
end

local gui = Instance.new("ScreenGui", game.CoreGui)
gui.Name = "Flshty_TrollHub"
gui.ResetOnSpawn = false
gui.DisplayOrder = 10000000000

local frame = Instance.new("Frame", gui)
frame.Size = UDim2.new(0, 300, 0, 160)
frame.Position = UDim2.new(0.5, -140, 0.5, -90)
frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
frame.Active = true
frame.Draggable = true

local corner = Instance.new("UICorner", frame)
corner.CornerRadius = UDim.new(0, 10)


local textbox = Instance.new("TextBox", frame)
textbox.PlaceholderText = "Code in Discord"
textbox.Text = ""
textbox.Size = UDim2.new(0.6, -10, 0, 30)
textbox.Position = UDim2.new(0.05, 0, 0.2, 0)
textbox.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
textbox.TextColor3 = Color3.fromRGB(255, 255, 255)
textbox.Font = Enum.Font.Code
textbox.TextScaled = true
Instance.new("UICorner", textbox)

local sendBtn = Instance.new("TextButton", frame)
sendBtn.Size = UDim2.new(0.3, 0, 0, 30)
sendBtn.Position = UDim2.new(0.65, 0, 0.2, 0)
sendBtn.Text = "Send"
sendBtn.BackgroundColor3 = Color3.fromRGB(80, 0, 0)
sendBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
sendBtn.Font = Enum.Font.Code
sendBtn.TextScaled = true
Instance.new("UICorner", sendBtn)

local orText = Instance.new("TextLabel", frame)
orText.Text = "Or"
orText.Size = UDim2.new(1, 0, 0, 35)
orText.Position = UDim2.new(0, 0, 0.35, 0)
orText.BackgroundTransparency = 1
orText.TextColor3 = Color3.fromRGB(150, 150, 150)
orText.Font = Enum.Font.Code
orText.TextScaled = true

local flshtyBtn = Instance.new("TextButton", frame)
flshtyBtn.Text = "Flshty hub"
flshtyBtn.Size = UDim2.new(0.8, 0, 0, 35)
flshtyBtn.Position = UDim2.new(0.1, 0, 0.65, 0)
flshtyBtn.BackgroundColor3 = Color3.fromRGB(120, 0, 120)
flshtyBtn.TextColor3 = Color3.fromRGB(0, 255, 255)
flshtyBtn.Font = Enum.Font.Arcade
flshtyBtn.TextScaled = true
Instance.new("UICorner", flshtyBtn)


-- Si le code est correct
sendBtn.MouseButton1Click:Connect(function()
	if textbox.Text == "Flshty_user" then
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Flashtynooo/Roblox/refs/heads/Game-hub-protection/Protection%20(discord%20hub)"))()
		gui:Destroy()
	end
end)

-- Flshty hub ex√©cution
flshtyBtn.MouseButton1Click:Connect(function()
gui:Destroy()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/Flashtynooo/Roblox/refs/heads/Every-hub-every-games/Intro"))()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/Flashtynooo/Roblox/refs/heads/Every-hub-every-games/Copy"))()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/Flashtynooo/Roblox/refs/heads/Every-hub-every-games/Script%20total%202"))()
	end)
