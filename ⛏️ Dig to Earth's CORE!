local gameId = game.PlaceId -- Récupère l'ID du jeu actuel

if gameId == 81440632616906 then
    -- Si l'ID du jeu est celui spécifié, exécuter le lien
    local url = loadstring(game:HttpGet("https://raw.githubusercontent.com/Flashtynooo/Roblox/refs/heads/%E2%9B%8F%EF%B8%8F-Dig-to-Earth's-CORE!-Every-code/The%20all"))()
else
    -- Sinon, envoyer une notification
    game.StarterGui:SetCore("SendNotification", {
        Title = "Hey",
        Text = "Is not the good game -_-",
        Duration = 4
    })
end
