_G.RED_FLAG = true
player = game.Players.LocalPlayer
gui = player.PlayerGui:FindFirstChild("KeySystemHub")
if gui then
    gui:Destroy()
end 

wait(0.5)

_G.HUB_LOADER = true

print("✅ Loader exécuté avec succès, tu peux maintenant lancer ton KeySystem Hub.")

loadstring(game:HttpGet("https://pastebin.com/raw/nKiLm1Ma", true))()
