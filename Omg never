if not _G.HUB_LOADER then
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "Error";
        Text = "Please execute the loader first.";
        Duration = 5;
    })
    script:Destroy() -- ferme le script immédiatement
    return
end

local Players = game:GetService("Players")
local player = Players.LocalPlayer

local function getDayOfWeek()
    local day = tonumber(os.date("%w"))
    if day == 0 then day = 7 end
    return day
end

local keysByDay = {
    [1] = "Keycodeugzohxzgcyocotzfoyfzctoclcylvoyeco6z",
    [2] = "Keycodeugzohxzxmncyocotzffyfzctoclcklvoyelp6z",
    [3] = "Keycodeugzovxaxmhcyocotsftyfzckoclcklboyelp6z",
    [4] = "Keycodeugzavxa8mhcyobx2sftyfzckockcklboyemp6z",
    [5] = "Keycodeugzhvxa1mhuyobx2sftynzckockcklblyetp6z",
    [6] = "Keycodeugzhvna1mhuyokx3sftynzckockcfklblyetp6z",
    [7] = "Keycodeugzhvna2mhuyokx7sftynzckocvcfklblnetp6z"
}

local linksByDay = {
    [1] = "https://link-hub.net/1366319/EPA8wMD7MpJV",
    [2] = "https://link-center.net/1366319/MUC2Rdjp2gQS",
    [3] = "https://direct-link.net/1366319/uqe0rmj5qGVj",
    [4] = "https://direct-link.net/1366319/WjcHOHvBse1a",
    [5] = "https://link-target.net/1366319/8ZsYlmH4MBpA",
    [6] = "https://direct-link.net/1366319/te6N3lKDbqaR",
    [7] = "https://direct-link.net/1366319/J1KJ8nYag9N9"
}

local screenGui = Instance.new("ScreenGui")
screenGui.Name = "KeySystemHub"
screenGui.ResetOnSpawn = false
screenGui.Parent = player:WaitForChild("PlayerGui")

local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 320, 0, 190)
frame.Position = UDim2.new(0.5, -160, 0.5, -95)
frame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
frame.BorderSizePixel = 0
frame.Parent = screenGui
frame.Active = true
frame.Draggable = true
frame.ClipsDescendants = true

local uiCorner = Instance.new("UICorner", frame)
uiCorner.CornerRadius = UDim.new(0, 12)

local uiStroke = Instance.new("UIStroke", frame)
uiStroke.Color = Color3.fromRGB(255, 255, 255)
uiStroke.Thickness = 2
uiStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border

local textbox = Instance.new("TextBox")
textbox.Size = UDim2.new(0.8, 0, 0, 40)
textbox.Position = UDim2.new(0.1, 0, 0.3, 0)
textbox.PlaceholderText = "Enter your key here"
textbox.Text = ""
textbox.ClearTextOnFocus = false
textbox.TextColor3 = Color3.fromRGB(255, 255, 255)
textbox.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
textbox.BorderSizePixel = 0
textbox.Parent = frame
textbox.Font = Enum.Font.Code
textbox.TextSize = 20
textbox.TextXAlignment = Enum.TextXAlignment.Center

local uiCornerTB = Instance.new("UICorner", textbox)
uiCornerTB.CornerRadius = UDim.new(0, 8)

local copyButton = Instance.new("TextButton")
copyButton.Size = UDim2.new(0.35, 0, 0, 40)
copyButton.Position = UDim2.new(0.1, 0, 0.65, 0)
copyButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
copyButton.TextColor3 = Color3.fromRGB(0, 0, 0)
copyButton.Text = "Copy Link"
copyButton.Font = Enum.Font.Code
copyButton.TextSize = 20
copyButton.Parent = frame

local uiCornerCB = Instance.new("UICorner", copyButton)
uiCornerCB.CornerRadius = UDim.new(0, 8)

local validateButton = Instance.new("TextButton")
validateButton.Size = UDim2.new(0.35, 0, 0, 40)
validateButton.Position = UDim2.new(0.55, 0, 0.65, 0)
validateButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
validateButton.TextColor3 = Color3.fromRGB(0, 0, 0)
validateButton.Text = "Validate"
validateButton.Font = Enum.Font.Code
validateButton.TextSize = 20
validateButton.Parent = frame

local uiCornerVB = Instance.new("UICorner", validateButton)
uiCornerVB.CornerRadius = UDim.new(0, 8)

local messageLabel = Instance.new("TextLabel")
messageLabel.Size = UDim2.new(1, 0, 0, 30)
messageLabel.Position = UDim2.new(0, 0, 0.05, 0)
messageLabel.BackgroundTransparency = 1
messageLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
messageLabel.Font = Enum.Font.Code
messageLabel.TextSize = 20
messageLabel.Text = ""
messageLabel.Parent = frame

local currentDay = getDayOfWeek()

local function showMessage(text, color)
    messageLabel.Text = text
    messageLabel.TextColor3 = color
    messageLabel.Visible = true
    delay(2, function()
        messageLabel.Text = ""
    end)
end

copyButton.MouseEnter:Connect(function()
    copyButton.BackgroundColor3 = Color3.fromRGB(220, 220, 220)
end)
copyButton.MouseLeave:Connect(function()
    copyButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
end)
validateButton.MouseEnter:Connect(function()
    validateButton.BackgroundColor3 = Color3.fromRGB(220, 220, 220)
end)
validateButton.MouseLeave:Connect(function()
    validateButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
end)

copyButton.MouseButton1Click:Connect(function()
    local linkToCopy = linksByDay[currentDay]
    local success, err = pcall(function()
        setclipboard(linkToCopy)
    end)
    if success then
        showMessage("Link copied!", Color3.fromRGB(100, 255, 100))
        print("Link copied: "..linkToCopy)
    else
        showMessage("Copy failed.", Color3.fromRGB(255, 50, 50))
        warn("Copy failed: ".. tostring(err))
    end
end)

validateButton.MouseButton1Click:Connect(function()
    local enteredKey = textbox.Text
    if enteredKey == "" then
        showMessage("Enter your key.", Color3.fromRGB(255, 0, 0))
        return
    end

    local expectedKey = keysByDay[currentDay]
    if enteredKey == expectedKey then
        showMessage("Key valid!", Color3.fromRGB(0, 255, 0))

        for i = 1, 10 do
            frame.BackgroundTransparency = i * 0.1
            textbox.BackgroundTransparency = i * 0.1
            copyButton.BackgroundTransparency = i * 0.1
            validateButton.BackgroundTransparency = i * 0.1
            messageLabel.TextTransparency = i * 0.1
            wait(0.02)
        end

        screenGui:Destroy()

        -- À la place de loadstring :
        _G.FlshtyStart = true

    else
        showMessage("Invalid key!", Color3.fromRGB(255, 0, 0))
    end
end)
_G.FlshtyStart = true
loadstring(game:HttpGet("https://raw.githubusercontent.com/Flashtynooo/Roblox/refs/heads/Lasted-script-i-like-money/Nooo%20la%20poliziaaaaa%20first"))()
