local function launchFreeHub()
    -- Destroy le hub existant imm√©diatement si il existe encore
    local existing = game.CoreGui:FindFirstChild("FlshtyKeyHub")
    if existing then
        existing:Destroy()
    end

    -- Lancement du Free Flshty Hub complet
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Flashtynooo/Roblox/refs/heads/Every-hub-every-games/Intro"))()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Flashtynooo/Roblox/refs/heads/Every-hub-every-games/Copy"))()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Flashtynooo/Roblox/refs/heads/Every-hub-every-games/Script%20total%202"))()
end

local function launchKeyHub()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Flashtynooo/Roblox/refs/heads/Flshty-hub-discord-user/Hub%20flshty%20discord%20user"))()
end

FreeButton.MouseButton1Click:Connect(function()
    launchFreeHub()
    MainFrame:Destroy()
end)

CopyButton.MouseButton1Click:Connect(function()
    if Clipboard then
        Clipboard("https://link-hub.net/1366319/7R789PjMo6Sf")
    end
end)

CodeBox.FocusLost:Connect(function(enter)
    if enter then
        if CodeBox.Text == "%zX7ÿöv_pTqyLÿö9kW3uF#tÿö2JrVbQx_s8MÿöZnE4dÿöLjA6cP" then
            launchKeyHub()
            MainFrame:Destroy()
        else
            CodeBox.Text = "‚ùå Code invalide"
            wait(1)
            CodeBox.Text = ""
        end
    end
end)
