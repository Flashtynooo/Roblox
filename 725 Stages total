local gameId = game.PlaceId -- Récupère l'ID du jeu actuel

if gameId == 8003084678 then
    -- Si l'ID du jeu est celui spécifié, exécuter le lien
    local url = "https://raw.githubusercontent.com/Flashtynooo/Roblox/refs/heads/Every-hub-every-games/Obby%20725%20stage"
    loadstring(game:HttpGet(url))() -- Exécute le script à partir du lien

else
    -- Sinon, envoyer une notification
    game.StarterGui:SetCore("SendNotification", {
        Title = "Hey",
        Text = "Is not the good game -_-",
        Duration = 4
    })
end
